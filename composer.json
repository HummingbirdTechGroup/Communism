{
  "name": "carlosv2/communism",
  "type": "library",
  "description": "Little utility to easily extract and inject properties from objects",
  "keywords": ["Reflection", "property", "test"],
  "license": "MIT",
  "authors": [
    {
      "name": "Carlos Ortega",
      "email": "carlosV2.0@gmail.com"
    }
  ],
  "autoload": {
    "files": ["src/bootstrap.php"],
    "psr-4": {
      "carlosV2\\Communism\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\carlosV2\\Communism\\": "spec"
    }
  },
  "require": {
    "php": "~5.3 || ~7.0"
  },
  "require-dev": {
    "phpspec/phpspec": "~2.5.5"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
